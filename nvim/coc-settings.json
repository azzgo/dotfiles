{
  "javascript.suggestionActions.enabled": false,
  "javascript.validate.enable": false,
  "javascript.suggest.enabled": false,
  "typescript.validate.enable": true,
  "vetur.format.enable": true,
  "git.addGBlameToVirtualText": true,
  "explorer.icon.enableVimDevicons": true,
  "vetur.languageFeatures.codeActions": true,
  "coc.preferences.enableMarkdown": false,
  "list.source.files.args": ["--hidden", "--files"],
  "explorer.file.hiddenRules": {
    "extensions": [
      "o",
      "a",
      "obj",
      "pyc"
    ],
    "filenames": [],
    "patternMatches": [
      "node_modules",
      "^\\."
    ]
  },
  "list.source.symbols.defaultOptions": [
    "--number-select",
    "--interactive",
    "--ignore-case"
  ],
  "list.source.grep.defaultOptions": [
    "--interactive",
    "--auto-preview"
  ],
  "list.insertMappings": {
    "<C-c>": "do:exit"
  },
  "list.normalMappings": {
    "<C-c>": "do:exit"
  },
  "git.enableGutters": true,
  "coc.preferences.extensionUpdateCheck": "daily"
}
